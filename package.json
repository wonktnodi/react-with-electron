  "private": true,
  "dependencies": {
    "electron": "^2.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
